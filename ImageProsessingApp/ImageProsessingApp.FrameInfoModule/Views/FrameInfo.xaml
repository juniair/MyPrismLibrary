<UserControl x:Class="ImageProsessingApp.FrameInfoModule.Views.FrameInfo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Label Content="{Binding Prosessor.LoopState}"  />
        <Label Content="{Binding Prosessor.FrameName}" Grid.Row="1" />
        <Label Content="{Binding Prosessor.FrameDuration}" Grid.Row="2" />
    </Grid>
</UserControl>
